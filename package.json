{
  "name": "impequid",
  "version": "0.0.2",
  "description": "Decentralized cloud.",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodekeract/impequid.git"
  },
  "scripts": {
    "start": "node build/server",
    "copy-client": "cp source/client/index.html build/client",
    "build-server": "babel --presets es2015 source/server -d build/server",
    "build-client": "webpack --config source/various/webpack.js ./source/client/index.jsx ./build/client/app.js",
    "watch-server": "nodemon --ignore build --ignore source/client --exec \"npm run build-server && npm run start\"",
    "watch-client": "nodemon -e css,html,jpg,js,jsx,png,scss --ignore build/ --ignore source/server --exec \"npm run build-client && npm run copy-client\""
  },
  "author": "Dodekeract",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dodekeract/impequid/issues"
  },
  "homepage": "https://github.com/dodekeract/impequid#readme",
  "dependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "koa": "^1.2.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "nodemon": "^1.9.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.1",
    "react-mini-router": "^2.0.0",
    "socket.io": "^1.4.5",
    "webpack": "^1.13.0"
  }
}
