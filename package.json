{
  "name": "impequid",
  "version": "0.0.2",
  "description": "Decentralized cloud.",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodekeract/impequid.git"
  },
  "scripts": {
    "bundle": "npm run bundle-main --silent && npm run bundle-authenticate --silent",
    "bundle-main": "webpack --progress --colors build/client/apps/main.js build/client/bundle/main.js",
    "bundle-authenticate": "webpack --progress --colors build/client/apps/authenticate.js build/client/bundle/authenticate.js",
    "build": "npm run remove --silent && npm run compile --silent && npm run copy --silent && npm run bundle --silent",
    "compile": "babel --plugins transform-async-to-generator --presets es2015,react source/ -d build/",
    "copy": "cp -r source/client/styles/ build/client/",
    "remove": "rm -rf build/",
    "start": "node build/server",
    "watch": "nodemon -e js,jsx,html,css --watch package.json --watch source/ --exec \"npm run build --silent && npm run start --silent\"",
    "watch-babel": "babel --plugins transform-async-to-generator --presets es2015,react source -d build/ --watch",
    "watch-bundle-authenticate": "webpack --progress --colors build/client/apps/authenticate.js build/client/bundle/authenticate.js --watch",
    "watch-bundle-main": "webpack --progress --colors build/client/apps/main.js build/client/bundle/main.js --watch",
    "watch-copy": "nodemon -e css --watch styles/ --exec \"cp -r source/client/styles/ build/client/\"",
    "watch-start": "nodemon -e js,jsx,html --watch build/ --exec \"spd-say kek --volume -75 && npm run start --silent\" --delay 250ms",
    "watch-sound": "nodemon -e js,jsx,html,css --watch package.json --watch source/ --exec \"spd-say build --volume -75 && npm run build --silent && spd-say done --volume -75 && npm run start --silent\"",
    "prepublish": "npm run build --silent",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags"
  },
  "author": "Dodekeract",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dodekeract/impequid/issues"
  },
  "homepage": "https://github.com/dodekeract/impequid#readme",
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "flux": "^2.1.1",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-generic-session": "^1.10.2",
    "koa-router": "^5.4.0",
    "koa-session-mongoose": "^1.0.0",
    "koa-static": "^2.0.0",
    "nodemon": "^1.9.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.1",
    "react-gcaptcha": "^3.0.1",
    "react-mini-router": "^2.0.0",
    "recaptcha-validator": "^1.0.7",
    "short-term-memory": "0.0.3",
    "socket.io": "^1.4.5",
    "validator": "^5.4.0",
    "webpack": "^1.13.0"
  }
}
